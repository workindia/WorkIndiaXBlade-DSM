{
  "name": "@workindia/figma-icon-exporter",
  "private": true,
  "version": "0.1.0",
  "description": "Figma plugin to export icon frames as React icon components",
  "license": "MIT",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "4.0.3",
    "@create-figma-plugin/utilities": "4.0.3",
    "jszip": "^3.10.1",
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "4.0.3",
    "@create-figma-plugin/tsconfig": "4.0.3",
    "@figma/plugin-typings": "1.121.0",
    "@types/node": "24.10.1",
    "typescript": "5.9.3"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "workindia-icon-exporter",
    "name": "WorkIndia DSM Icon Exporter",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
