{
  "name": "@workindia/dsm-mcp",
  "sourceRoot": "packages/mcp/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/mcp"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "packages/mcp"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:coverage",
        "cwd": "packages/mcp"
      }
