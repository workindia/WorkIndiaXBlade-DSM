    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "packages/mcp"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint:fix",
        "cwd": "packages/mcp"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run typecheck",
        "cwd": "packages/mcp"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run clean",
        "cwd": "packages/mcp"
      }
    }
